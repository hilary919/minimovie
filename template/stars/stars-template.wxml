<template name="stars-template">
  <view class="stars-container">
    <view class="stars">
      <block wx:for="{{stars}}" wx:for-item="star" wx:key="key">
        <image wx:if="{{star}}" src="/images/icon/star.png"></image>
        <image wx:else src="/images/icon/none-star.png"></image>
      </block>
    </view>
    <text>{{rating}}</text>
  </view>
</template>